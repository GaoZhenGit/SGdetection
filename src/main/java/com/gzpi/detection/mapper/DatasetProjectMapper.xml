<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gzpi.detection.mapper.DatasetProjectMapper">
    <select id="getProjectById" parameterType="String" resultType="com.gzpi.detection.bean.DatasetProject">
        SELECT id, name, type, clip_size as clipSize FROM project WHERE id=#{id}
    </select>
    <select id="getAllProjects" resultType="com.gzpi.detection.bean.DatasetProject">
        SELECT id, name, type, clip_size as clipSize FROM project
    </select>
    <insert id="save" parameterType="com.gzpi.detection.bean.DatasetProject">
        insert into project(id, name, type, clip_size) VALUES (#{id}, #{name}, #{type}, #{clipSize})
    </insert>
    <delete id="delete" parameterType="String">
        delete from project WHERE id=#{id}
    </delete>
</mapper>